{
  "server": {
    "name": "debate-consensus-mcp-test",
    "version": "1.0.0",
    "port": 3001,
    "host": "localhost"
  },
  "api": {
    "openrouter": {
      "baseUrl": "https://openrouter.ai/api/v1/chat/completions",
      "maxRetries": 1,
      "retryDelay": 100,
      "defaultTemperature": 0.1,
      "defaultMaxTokens": 1024,
      "timeout": 5000
    },
    "anthropic": {
      "baseUrl": "https://api.anthropic.com/v1/messages",
      "maxRetries": 1,
      "retryDelay": 100,
      "defaultTemperature": 0.1,
      "defaultMaxTokens": 1024,
      "timeout": 5000
    }
  },
  "models": {
    "kimi": {
      "id": "moonshot/kimi-k2-latest",
      "name": "Kimi K2 Test",
      "provider": "openrouter",
      "capabilities": ["reasoning", "analysis"],
      "maxTokens": 1024,
      "temperature": 0.1
    },
    "qwen": {
      "id": "qwen/qwen-2.5-coder-32b-instruct",
      "name": "Qwen Coder Test",
      "provider": "openrouter",
      "capabilities": ["coding", "analysis"],
      "maxTokens": 1024,
      "temperature": 0.1
    },
    "gemini": {
      "id": "google/gemini-2.0-flash-thinking-exp-1219:free",
      "name": "Gemini Flash Thinking Test",
      "provider": "openrouter",
      "capabilities": ["reasoning", "analysis"],
      "maxTokens": 1024,
      "temperature": 0.1
    },
    "claude": {
      "id": "claude-3-5-sonnet-20241022",
      "name": "Claude 3.5 Sonnet Test",
      "provider": "anthropic",
      "capabilities": ["reasoning", "analysis", "synthesis"],
      "maxTokens": 1024,
      "temperature": 0.1
    }
  },
  "debate": {
    "maxRounds": 2,
    "minParticipants": 2,
    "maxParticipants": 3,
    "consensusThreshold": 0.6,
    "timeoutMs": 10000
  },
  "logging": {
    "level": "debug",
    "format": "simple",
    "directory": "./logs/test",
    "maxFiles": 5,
    "maxSize": "1mb"
  },
  "features": {
    "history": true,
    "logging": true,
    "validation": true,
    "caching": false,
    "metrics": true
  },
  "security": {
    "rateLimiting": {
      "windowMs": 60000,
      "maxRequests": 1000
    },
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3001"]
    }
  }
}